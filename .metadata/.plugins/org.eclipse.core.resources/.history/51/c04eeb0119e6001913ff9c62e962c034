package Inicio;

import java.sql.SQLException;

public class ConsultorBdD {
	public ConsultorBdD() {
		
	}
	
	public boolean chequearAdministrador (String password) throws SQLException {
		return java.sql.DriverManager.getConnection("jdbc:mysql://localhost:3306/vuelos?&user=admin&password=" + password) != null;
	}
	public boolean chequearEmpleado(String legajo, String password) {
		return true;
	}
}
